  pod 'RxCocoa',    '~> 4.0'
  
  target 'SecondSampleTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SecondSampleUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
